//Leetcode
//java

class Solution {
    public String toLowerCase(String str) {
        String s = new String();
        for(int i =0;i<str.length();i++){
            char a = str.charAt(i);
            if(a>=65 && a<=90){
                int b =a+32;
                a = (char)b;
            }
           s = s+a;
        }
        
        return s;
    }
}
