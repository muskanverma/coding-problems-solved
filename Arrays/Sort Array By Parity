//Leetcode

class Solution {
public:
    vector<int> sortArrayByParity(vector<int>& arr) {
     
        
        vector<int>ans;
        for(int i =0;i<arr.size();i++){
            if(arr[i]%2==0){
                ans.push_back(arr[i]);
            }
        }
        for(int i =0;i<arr.size();i++){
            if(arr[i]%2!=0){
                ans.push_back(arr[i]);
            }
        }
        return ans;
        
    }
};
